        android:background="@color/colorAccent" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ms12"
        android:id="@+id/imageButton32"
        android:layout_below="@+id/imageButton31"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="12dp"
        android:background="@color/colorAccent" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ms9"
        android:id="@+id/imageButton29"
        android:layout_marginBottom="214dp"
        android:background="@color/colorAccent"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/imageButton32"
        android:layout_alignStart="@+id/imageButton32" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ms10"
        android:id="@+id/imageButton30"
        android:layout_marginTop="73dp"
        android:background="@color/colorAccent"
        android:layout_alignTop="@+id/imageButton29"
        android:layout_alignLeft="@+id/imageButton29"
        android:layout_alignStart="@+id/imageButton29" />
</RelativeLayout>
